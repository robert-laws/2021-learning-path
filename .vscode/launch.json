{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Basic Chrome Debugger",
      "url": "http://localhost:8000/_debugging-example/index.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome Form",
      "url": "http://localhost:8000/_debugging-example/simpleForm.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "React Debug",
      "url": "http://localhost:8000/_debugging-example/index.html",
      "request": "launch",
      "type": "chrome",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "../*": "${webRoot}/*"
      }
    }
  ]
}
